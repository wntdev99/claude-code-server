{
  "name": "claude-code-server",
  "version": "0.1.0",
  "private": true,
  "description": "Claude Code Server - Web-based agent management platform",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @claude-code-server/web dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "db:generate": "pnpm --filter @claude-code-server/core db:generate",
    "db:migrate": "pnpm --filter @claude-code-server/core db:migrate",
    "db:push": "pnpm --filter @claude-code-server/core db:push",
    "clean": "pnpm -r clean"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
